x(index);\n        setIsPlaying(true);\n    }\n\n    function togglePlay() {\n        setIsPlaying(!isPlaying);\n    }\n\n    function setPlayingState(state: boolean) {\n        setIsPlaying(state);\n    }\n    return (\n        <PlayerContext.Provider value={{\n            episodeList,\n            currentEpisodeIndex,\n            play,\n            isPlaying,\n            togglePlay,\n            setPlayingState\n        }}>\n            {children}\n        </PlayerContext.Provider>\n    )\n}"]},"metadata":{},"sourceType":"module"}